{
  "name": "ethery",
  "icon": "media/icon.png",
  "author": "Rabiloo, Inc",
  "version": "1.1.34",
  "repository": {
    "type": "git",
    "url": "https://github.com/rabiloo/ethery"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "bugs": {
    "url": "https://github.com/rabiloo/ethery/issues",
    "email": "duydq@rabiloo.com"
  },
  "homepage": "https://rabiloo.com",
  "qna": "https://github.com/rabiloo/ethery/issues/new/choose",
  "license": "Apache-2.0",
  "displayName": "Ethery – Coding Beyond Limits",
  "pricing": "Free",
  "description": "Code Smarter, Not Harder",
  "publisher": "Rabiloo",
  "engines": {
    "vscode": "^1.70.0",
    "node": ">=20.19.0"
  },
  "engine-strict": true,
  "galleryBanner": {
    "color": "#1E1E1E",
    "theme": "dark"
  },
  "categories": [
    "AI",
    "Chat",
    "Programming Languages",
    "Education",
    "Machine Learning",
    "Snippets"
  ],
  "keywords": [
    "chatgpt",
    "github",
    "copilot",
    "claude",
    "sonnet",
    "mistral",
    "codestral",
    "codegpt",
    "ai",
    "llama"
  ],
  "activationEvents": [
    "onUri",
    "onStartupFinished",
    "onView:continueGUIView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "filenames": [
          "config.json",
          ".continuerc.json"
        ],
        "id": "jsonc"
      },
      {
        "id": "promptLanguage",
        "extensions": [
          ".prompt"
        ],
        "aliases": [
          "Prompt Language"
        ],
        "configuration": "./prompt-file-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "promptLanguage",
        "scopeName": "source.prompt",
        "path": "./media/prompt.tmLanguage.json"
      }
    ],
    "configuration": {
      "title": "Ethery",
      "properties": {
        "ethery.telemetryEnabled": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Ethery collects anonymous usage data, cleaned of PII, to help us improve the product for our users. Read more  at [continue.dev › Telemetry](https://docs.continue.dev/telemetry)."
        },
        "ethery.showInlineTip": {
          "type": "boolean",
          "default": true,
          "description": "Show inline suggestion to use the Ethery keyboard shortcuts (e.g. \"Cmd/Ctrl L to select code, Cmd/Ctrl I to edit\")."
        },
        "ethery.disableQuickFix": {
          "type": "boolean",
          "default": false,
          "description": "Disable the quick fix feature."
        },
        "ethery.enableQuickActions": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Enable the experimental Quick Actions feature. Read our walkthrough to learn about configuration and how to share feedback: [continue.dev › Walkthrough: Quick Actions (experimental)](https://docs.continue.dev/features/quick-actions)"
        },
        "ethery.enableTabAutocomplete": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable Ethery's tab autocomplete feature. Read our walkthrough to learn about configuration and how to share feedback: [continue.dev › Walkthrough: Tab Autocomplete (beta)](https://docs.continue.dev/features/tab-autocomplete)"
        },
        "ethery.pauseTabAutocompleteOnBattery": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Pause Ethery's tab autocomplete feature when your battery is low."
        },
        "ethery.pauseCodebaseIndexOnStart": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Pause Ethery's codebase index on start."
        },
        "ethery.enableConsole": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Enable a console to log and explore model inputs and outputs. It can be found in the bottom panel."
        },
        "ethery.remoteConfigServerUrl": {
          "type": "string",
          "default": null,
          "markdownDescription": "If your team is set up to use shared configuration, enter the server URL here and your user token below to enable automatic syncing."
        },
        "ethery.userToken": {
          "type": "string",
          "default": null,
          "markdownDescription": "If your team is set up to use shared configuration, enter your user token here and your server URL above to enable automatic syncing."
        },
        "ethery.remoteConfigSyncPeriod": {
          "type": "number",
          "default": 60,
          "description": "The period of time in minutes between automatic syncs."
        }
      }
    },
    "commands": [
      {
        "command": "ethery.applyCodeFromChat",
        "category": "Ethery",
        "title": "Apply code from chat",
        "group": "Ethery"
      },
      {
        "command": "ethery.acceptDiff",
        "category": "Ethery",
        "title": "Accept Diff",
        "group": "Ethery"
      },
      {
        "command": "ethery.rejectDiff",
        "category": "Ethery",
        "title": "Reject Diff",
        "group": "Ethery",
        "icon": "$(stop)"
      },
      {
        "command": "ethery.acceptVerticalDiffBlock",
        "category": "Ethery",
        "title": "Accept Vertical Diff Block",
        "group": "Ethery"
      },
      {
        "command": "ethery.rejectVerticalDiffBlock",
        "category": "Ethery",
        "title": "Reject Vertical Diff Block",
        "group": "Ethery"
      },
      {
        "command": "ethery.focusEdit",
        "category": "Ethery",
        "title": "Generate Code",
        "group": "Ethery"
      },
      {
        "command": "ethery.focusContinueInput",
        "category": "Ethery",
        "title": "Add Highlighted Code to Context and Clear Chat",
        "group": "Ethery"
      },
      {
        "command": "ethery.focusContinueInputWithoutClear",
        "category": "Ethery",
        "title": "Add Highlighted Code to Context",
        "group": "Ethery"
      },
      {
        "command": "ethery.debugTerminal",
        "category": "Ethery",
        "title": "Debug Terminal",
        "group": "Ethery"
      },
      {
        "command": "ethery.exitEditMode",
        "category": "Ethery",
        "title": "Exit Edit Mode",
        "group": "Ethery"
      },
      {
        "command": "ethery.toggleFullScreen",
        "category": "Ethery",
        "title": "Open in new window",
        "icon": "$(link-external)",
        "group": "Ethery"
      },
      {
        "command": "ethery.openConfigPage",
        "category": "Ethery",
        "title": "Open Settings",
        "icon": "$(gear)",
        "group": "Ethery"
      },
      {
        "command": "ethery.toggleTabAutocompleteEnabled",
        "category": "Ethery",
        "title": "Toggle Autocomplete Enabled",
        "group": "Ethery"
      },
      {
        "command": "ethery.selectFilesAsContext",
        "category": "Ethery",
        "title": "Select Files as Context",
        "group": "Ethery"
      },
      {
        "command": "ethery.newSession",
        "category": "Ethery",
        "title": "New Session",
        "icon": "$(add)",
        "group": "Ethery"
      },
      {
        "command": "ethery.viewHistory",
        "category": "Ethery",
        "title": "View History",
        "icon": "$(history)",
        "group": "Ethery"
      },
      {
        "command": "ethery.viewLogs",
        "category": "Ethery",
        "title": "View Logs",
        "group": "Ethery"
      },
      {
        "command": "ethery.clearConsole",
        "category": "Ethery",
        "title": "Clear Console",
        "icon": "$(clear-all)",
        "group": "Ethery"
      },
      {
        "command": "ethery.navigateTo",
        "category": "Ethery",
        "title": "Navigate to a path",
        "group": "Ethery"
      },
      {
        "command": "ethery.writeCommentsForCode",
        "category": "Ethery",
        "title": "Write Comments for this Code",
        "group": "Ethery"
      },
      {
        "command": "ethery.writeDocstringForCode",
        "category": "Ethery",
        "title": "Write a Docstring for this Code",
        "group": "Ethery"
      },
      {
        "command": "ethery.fixCode",
        "category": "Ethery",
        "title": "Fix this Code",
        "group": "Ethery"
      },
      {
        "command": "ethery.optimizeCode",
        "category": "Ethery",
        "title": "Optimize this Code",
        "group": "Ethery"
      },
      {
        "command": "ethery.fixGrammar",
        "category": "Ethery",
        "title": "Fix Grammar / Spelling",
        "group": "Ethery"
      },
      {
        "command": "ethery.codebaseForceReIndex",
        "category": "Ethery",
        "title": "Codebase Force Re-Index",
        "group": "Ethery"
      },
      {
        "command": "ethery.rebuildCodebaseIndex",
        "category": "Ethery",
        "title": "Rebuild codebase index",
        "group": "Ethery"
      },
      {
        "command": "ethery.docsIndex",
        "category": "Ethery",
        "title": "Docs Index",
        "group": "Ethery"
      },
      {
        "command": "ethery.docsReIndex",
        "category": "Ethery",
        "title": "Docs Force Re-Index",
        "group": "Ethery"
      },
      {
        "command": "ethery.focusContinueInput",
        "category": "Ethery",
        "title": "Focus Ethery Chat",
        "group": "Ethery"
      }
    ],
    "keybindings": [
      {
        "command": "ethery.focusContinueInput",
        "mac": "cmd+l",
        "key": "ctrl+l"
      },
      {
        "command": "ethery.focusContinueInputWithoutClear",
        "mac": "cmd+shift+l",
        "key": "ctrl+shift+l"
      },
      {
        "command": "ethery.acceptDiff",
        "mac": "shift+cmd+enter",
        "key": "shift+ctrl+enter",
        "when": "ethery.diffVisible"
      },
      {
        "command": "ethery.rejectDiff",
        "mac": "shift+cmd+backspace",
        "key": "shift+ctrl+backspace",
        "when": "ethery.diffVisible"
      },
      {
        "command": "ethery.rejectDiff",
        "mac": "cmd+z",
        "key": "ctrl+z",
        "when": "ethery.diffVisible"
      },
      {
        "command": "ethery.quickEditHistoryUp",
        "mac": "up",
        "key": "up",
        "when": "false && ethery.quickEditHistoryFocused"
      },
      {
        "command": "ethery.quickEditHistoryDown",
        "mac": "down",
        "key": "down",
        "when": "false && ethery.quickEditHistoryFocused"
      },
      {
        "command": "ethery.acceptVerticalDiffBlock",
        "mac": "alt+cmd+y",
        "key": "alt+ctrl+y"
      },
      {
        "command": "ethery.rejectVerticalDiffBlock",
        "mac": "alt+cmd+n",
        "key": "alt+ctrl+n"
      },
      {
        "command": "ethery.focusEdit",
        "title": "Edit code with natural language",
        "mac": "cmd+i",
        "key": "ctrl+i"
      },
      {
        "command": "ethery.exitEditMode",
        "mac": "escape",
        "key": "escape",
        "when": "ethery.inEditMode && editorFocus"
      },
      {
        "command": "ethery.debugTerminal",
        "mac": "cmd+shift+r",
        "key": "ctrl+shift+r"
      },
      {
        "command": "ethery.toggleFullScreen",
        "mac": "cmd+k cmd+m",
        "key": "ctrl+k ctrl+m",
        "when": "!terminalFocus"
      },
      {
        "command": "ethery.toggleTabAutocompleteEnabled",
        "mac": "cmd+k cmd+a",
        "key": "ctrl+k ctrl+a",
        "when": "!terminalFocus"
      },
      {
        "command": "ethery.applyCodeFromChat",
        "mac": "alt+a",
        "key": "alt+a"
      }
    ],
    "submenus": [
      {
        "id": "ethery.continueSubMenu",
        "label": "Ethery"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "ethery.focusContinueInput"
        },
        {
          "command": "ethery.focusContinueInputWithoutClear"
        },
        {
          "command": "ethery.debugTerminal"
        },
        {
          "command": "ethery.toggleFullScreen"
        },
        {
          "command": "ethery.newSession"
        }
      ],
      "editor/context": [
        {
          "submenu": "ethery.continueSubMenu",
          "group": "0_acontinue"
        }
      ],
      "editor/title/run": [
        {
          "command": "ethery.rejectDiff",
          "group": "Ethery",
          "when": "ethery.streamingDiff"
        }
      ],
      "ethery.continueSubMenu": [
        {
          "command": "ethery.focusContinueInputWithoutClear",
          "group": "Ethery",
          "when": "editorHasSelection"
        },
        {
          "command": "ethery.writeCommentsForCode",
          "group": "Ethery",
          "when": "editorHasSelection && !editorReadonly"
        },
        {
          "command": "ethery.writeDocstringForCode",
          "group": "Ethery",
          "when": "editorHasSelection && !editorReadonly"
        },
        {
          "command": "ethery.fixCode",
          "group": "Ethery",
          "when": "editorHasSelection && !editorReadonly"
        },
        {
          "command": "ethery.optimizeCode",
          "group": "Ethery",
          "when": "editorHasSelection && !editorReadonly"
        },
        {
          "command": "ethery.fixGrammar",
          "group": "Ethery",
          "when": "editorHasSelection && editorLangId == 'markdown' && !editorReadonly"
        }
      ],
      "explorer/context": [
        {
          "command": "ethery.selectFilesAsContext",
          "group": "1_debug@1"
        }
      ],
      "view/title": [
        {
          "command": "ethery.newSession",
          "group": "navigation@1",
          "when": "view == ethery.continueGUIView"
        },
        {
          "command": "ethery.viewHistory",
          "group": "navigation@2",
          "when": "view == ethery.continueGUIView"
        },
        {
          "command": "ethery.toggleFullScreen",
          "group": "navigation@3",
          "when": "view == ethery.continueGUIView"
        },
        {
          "command": "ethery.openConfigPage",
          "group": "navigation@4",
          "when": "view == ethery.continueGUIView"
        },
        {
          "command": "ethery.clearConsole",
          "group": "navigation@1",
          "when": "view == ethery.continueConsoleView"
        }
      ],
      "editor/title": [
        {
          "command": "ethery.newSession",
          "group": "navigation@1",
          "when": "activeWebviewPanelId == ethery.continueGUIView"
        },
        {
          "command": "ethery.viewHistory",
          "group": "navigation@2",
          "when": "activeWebviewPanelId == ethery.continueGUIView"
        }
      ],
      "terminal/context": [
        {
          "command": "ethery.debugTerminal",
          "group": "navigation@top"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "continue",
          "title": "Ethery",
          "icon": "media/sidebar-icon.png"
        }
      ],
      "panel": [
        {
          "id": "continueConsole",
          "title": "Ethery Console",
          "icon": "$(window)"
        }
      ]
    },
    "views": {
      "continue": [
        {
          "type": "webview",
          "id": "ethery.continueGUIView",
          "name": "Ethery",
          "icon": "media/sidebar-icon.png",
          "visibility": "visible"
        }
      ],
      "continueConsole": [
        {
          "type": "webview",
          "id": "ethery.continueConsoleView",
          "name": "Ethery Console",
          "icon": "$(window)",
          "visibility": "visible",
          "when": "config.ethery.enableConsole"
        }
      ]
    },
    "jsonValidation": [
      {
        "fileMatch": "**/.ethery*/config.json",
        "url": "./config_schema.json"
      },
      {
        "fileMatch": ".continuerc.json",
        "url": "./continue_rc_schema.json"
      },
      {
        "fileMatch": "**/config.yaml",
        "url": "./config-yaml-schema.json"
      }
    ]
  },
  "scripts": {
    "esbuild-base": "node scripts/esbuild.js",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "esbuild-notify": "npm run esbuild-base -- --sourcemap --notify",
    "esbuild:visualize": "esbuild-visualizer --metadata ./build/meta.json --filename ./build/stats.html --open",
    "tsc": "tsc -p ./",
    "tsc:check": "tsc -p ./ --noEmit",
    "tsc-watch": "tsc -watch -p ./",
    "rebuild": "electron-rebuild -v 19.1.8 node-pty",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint . --ext ts --fix",
    "build-test": "npm run esbuild && node esbuild.test.mjs",
    "test": "npm run build-test && node ./out/runTestOnVSCodeHost.js",
    "quick-test": "npm run build-test && node ./out/runTestOnVSCodeHost.js",
    "write-build-timestamp": "node scripts/write-build-timestamp.js",
    "prepackage": "node scripts/prepackage.js",
    "package": "node scripts/package.js",
    "package-all": "node scripts/package-all.js",
    "package:pre-release": "node scripts/package.js --pre-release",
    "build:rust": "cargo-cp-artifact -ac sync sync.node -- cargo build --manifest-path ../../sync/Cargo.toml --message-format=json-render-diagnostics",
    "build-debug:rust": "npm run build:rust --",
    "build-release:rust": "npm run build:rust -- --release",
    "e2e:compile": "tsc -p ./tsconfig.e2e.json",
    "e2e:build": "npm --prefix ../../gui run build && npm run package",
    "e2e:create-storage": "mkdir -p ./e2e/storage",
    "e2e:get-chromedriver": "extest get-chromedriver --storage ./e2e/storage --code_version 1.95.0",
    "e2e:get-vscode": "extest get-vscode --storage ./e2e/storage --code_version 1.95.0",
    "e2e:sign-vscode": "codesign --entitlements entitlements.plist --deep --force -s - './e2e/storage/Visual Studio Code.app'",
    "e2e:copy-vsix": "chmod +x ./e2e/get-latest-vsix.sh && bash ./e2e/get-latest-vsix.sh",
    "e2e:install-vsix": "extest install-vsix -f ./e2e/vsix/ethery.vsix --extensions_dir ./e2e/.test-extensions --storage ./e2e/storage",
    "e2e:install-extensions": "extest install-from-marketplace ms-vscode-remote.remote-ssh --extensions_dir ./e2e/.test-extensions --storage ./e2e/storage && extest install-from-marketplace ms-vscode-remote.remote-containers --extensions_dir ./e2e/.test-extensions --storage ./e2e/storage && extest install-from-marketplace ms-vscode-remote.remote-wsl --extensions_dir ./e2e/.test-extensions --storage ./e2e/storage",
    "e2e:test": "extest run-tests ${TEST_FILE:-'./e2e/_output/tests/*.test.js'} --code_settings settings.json --extensions_dir ./e2e/.test-extensions --storage ./e2e/storage --log_level INFO",
    "e2e:clean": "rm -rf ./e2e/_output ./e2e/storage",
    "e2e:all": "npm run e2e:build && npm run e2e:compile && npm run e2e:create-storage && npm run e2e:get-chromedriver && npm run e2e:get-vscode && npm run e2e:sign-vscode && npm run e2e:copy-vsix && npm run e2e:install-vsix && npm run e2e:install-extensions && CONTINUE_GLOBAL_DIR=e2e/test-continue npm run e2e:test && npm run e2e:clean",
    "e2e:recompile-extension": "npm run package && npm run e2e:compile && npm run e2e:copy-vsix && npm run e2e:install-vsix && npm run e2e:install-extensions && CONTINUE_GLOBAL_DIR=e2e/test-continue npm run e2e:test && npm run e2e:clean",
    "e2e:rebuild-gui": "rm -rf gui && cp -r ../../gui/dist gui && npm run package && npm run e2e:copy-vsix && npm run e2e:install-vsix && npm run e2e:install-extensions && CONTINUE_GLOBAL_DIR=e2e/test-continue npm run e2e:test && npm run e2e:clean",
    "e2e:quick": "npm run e2e:compile && CONTINUE_GLOBAL_DIR=e2e/test-continue npm run e2e:test && npm run e2e:clean",
    "e2e:ci:download": "npm run e2e:create-storage && npm run e2e:get-chromedriver && npm run e2e:get-vscode",
    "e2e:ci:run": "npm run e2e:compile && npm run e2e:copy-vsix && npm run e2e:install-vsix && npm run e2e:install-extensions && CONTINUE_GLOBAL_DIR=e2e/test-continue npm run e2e:test",
    "e2e:ci:run-yaml": "npm run e2e:compile && npm run e2e:copy-vsix && npm run e2e:install-vsix && npm run e2e:install-extensions && CONTINUE_GLOBAL_DIR=e2e/test-continue-yaml npm run e2e:test"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@nestjs/common": "^11.0.16",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/chai": "^5.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/follow-redirects": "^1.14.4",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "16.x",
    "@types/react-dom": "^18.2.4",
    "@types/request": "^2.48.8",
    "@types/vscode": "1.70",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@vscode/vsce": "^2.22.0",
    "cargo-cp-artifact": "^0.1",
    "chai": "^4.5.0",
    "esbuild": "0.17.19",
    "esbuild-visualizer": "^0.6.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "json-schema-to-typescript": "^12.0.0",
    "mocha": "^10.4.0",
    "ovsx": "^0.8.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.6.3",
    "vite": "^4.5.14",
    "vscode-extension-tester": "^8.14.1"
  },
  "dependencies": {
    "@continuedev/config-types": "^1.0.14",
    "@continuedev/fetch": "^1.0.3",
    "@electron/rebuild": "^3.2.10",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/node-fetch": "^2.6.11",
    "@types/uuid": "^9.0.8",
    "@vscode/ripgrep": "^1.15.9",
    "@vscode/test-electron": "^2.3.9",
    "axios": "^1.2.5",
    "core": "file:../../core",
    "cors": "^2.8.5",
    "dbinfoz": "^0.14.0",
    "diff": "^7.0.0",
    "downshift": "^7.6.0",
    "esbuild": "0.17.19",
    "express": "^4.18.2",
    "fkill": "^8.1.0",
    "follow-redirects": "^1.15.4",
    "handlebars": "^4.7.8",
    "highlight.js": "^11.7.0",
    "highlightable": "^1.3.0-beta.0",
    "http-proxy": "^1.18.1",
    "http-proxy-agent": "^7.0.0",
    "http-proxy-middleware": "^2.0.9",
    "https-proxy-agent": "^7.0.2",
    "ignore": "^5.3.0",
    "jsdom": "^24.0.0",
